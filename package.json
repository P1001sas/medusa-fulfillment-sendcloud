{
  "name": "@hifive-dev/medusa-fulfillment-sendcloud",
  "version": "1.1.0",
  "description": "A MedusaJS plugin integrating the SendCloud platform used for managing shipping duties.",
  "author": "Hifive Dev (https://hifive.dev)",
  "license": "MIT",
  "keywords": [
    "sendcloud",
    "shipping",
    "tracking",
    "medusajs",
    "fulfilment",
    "ecommerce",
    "headless",
    "medusa",
    "plugin",
    "integration",
    "medusa-plugin-shipping",
    "medusa-plugin-storefront",
    "medusa-plugin-admin",
    "medusa-plugin"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production tsc --build",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "test": "jest --passWithNoTests src",
    "build": "tsc -p ./tsconfig.server.json && medusa-admin bundle",
    "watch": "tsc --watch"
  },
  "peerDependencies": {
    "@medusajs/medusa": "1.20.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.13.0"
  },
  "dependencies": {
    "@medusajs/admin": "7.1.11",
    "@medusajs/cache-inmemory": "^1.8.10",
    "@medusajs/cache-redis": "^1.9.0",
    "@medusajs/event-bus-local": "^1.9.8",
    "@medusajs/event-bus-redis": "^1.8.11",
    "@medusajs/file-local": "^1.0.3",
    "@medusajs/medusa": "1.20.2",
    "@tanstack/react-query": "5.20.1",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "16.4.2",
    "express": "^4.18.2",
    "medusa-interfaces": "^1.3.8",
    "medusa-react": "^9.0.15",
    "prism-react-renderer": "^2.3.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@medusajs/medusa-cli": "^1.3.22",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hifive-dev/medusa-fulfillment-sendcloud.git"
  },
  "bugs": {
    "url": "https://github.com/hifive-dev/medusa-fulfillment-sendcloud/issues"
  },
  "homepage": "https://github.com/hifive-dev/medusa-fulfillment-sendcloud#readme"
}
